{
  "name": "Wishlist",
  "version": "1.1.8",
  "manifest_version": 2,
  "description": "Add stuff to your wishlist",
  "homepage_url": "https://github.com/akhilgupta1093/opinionated",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "https://*/*",
    "storage",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["inject.js"]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Wishlist"
  },
  "web_accessible_resources": [
    "images/*.png",
    "*.jpg",
    "*.JPG"
  ]
}
